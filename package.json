{
  "name": "generate-ts-docs",
  "version": "0.0.5",
  "description": "Utilities to parse type information and JSDoc comments from TypeScript functions, and render documentation as Markdown",
  "keywords": [
    "doc",
    "docs",
    "documentation",
    "jsdoc",
    "markdown",
    "typedoc",
    "typescript"
  ],
  "author": "Yuan Qing Lim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/generate-ts-docs.git"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn run clean && tsc",
    "docs": "yarn run example && markdown-interpolate README.md",
    "clean": "rimraf '*.log' .nyc_output build coverage lib",
    "example": "cd example && ts-node generate-ts-docs.ts > README.md",
    "fix": "eslint --fix '{scripts,src}/**/*.ts'",
    "lint": "eslint '{scripts,src}/**/*.ts'",
    "prepublishOnly": "yarn run build",
    "reset": "yarn run clean && rimraf node_modules yarn.lock && yarn install",
    "test": "tap 'src/**/__tests__/**/*.ts' --coverage-report html --coverage-report text --jobs-auto --no-browser --reporter terse",
    "watch": "yarn run clean && tsc --watch"
  },
  "dependencies": {
    "github-slugger": "^1.3.0",
    "globby": "^11.0.1",
    "typescript": "*"
  },
  "devDependencies": {
    "@types/github-slugger": "^1.3.0",
    "@types/node": "^14.14.9",
    "@types/tap": "^14.10.1",
    "eslint": "^7.14.0",
    "eslint-config-yuanqing": "^0.0.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "markdown-interpolate": "^0.0.7",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "tap": "^14.11.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "tap/@types/react": "17.0.0"
  },
  "eslintConfig": {
    "extends": "eslint-config-yuanqing"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ]
  },
  "prettier": "eslint-config-yuanqing/prettier"
}
